{
  "name": "bit-stream",
  "version": "1.0.0",
  "description": "A decoder and encoder that allow you to manipulate bits in nanoseconds.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist/* && rollup -c",
    "test": "jest --watchAll --roots ./tests",
    "benchmark": "node ./benchmark/index.js"
  },
  "files": [
    "dist/index.cjs",
    "dist/index.mjs",
    "dist/index.min.js",
    "index.d.ts"
  ],
  "keywords": [
    "base64",
    "hex",
    "utf16",
    "utf8",
    "encoder",
    "decoder",
    "bit",
    "stream"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rayyamhk/bit-stream"
  },
  "homepage": "https://github.com/rayyamhk/bit-stream#readme",
  "bugs": {
    "url": "https://github.com/rayyamhk/bit-stream/issues"
  },
  "author": "Ray Yam",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "benchmark": "^2.1.4",
    "jest": "^28.1.0",
    "rollup": "^2.75.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
